{
    "presets": [
      ["@babel/preset-env",
    { "corejs": "3.1.3",
      "useBuiltIns": "usage" }]
    ],
    "plugins": [
       "@babel/plugin-proposal-export-namespace-from",
      [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
          "@api": "./src/api/",
          "@middleware": "./src/api/middleware",
          "@utils": "./src/utils",
          "@root": "./src",
          "@models": "./src/database/model/gatewayModel.js",
          "@app": "./src/app",
          "@database": "./src/database"
        }
      }
    ]]
  }
  